{
  "name": "snapwagon",
  "version": "0.0.1",
  "description": "a saas plugin that allows smbs to quickly offer coupons to their customers",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "build": "npm run build:css && npm run build:esModules && npm run build:var",
    "build:esModules": "npm run clean:esModules && BABEL_ENV=production ./node_modules/.bin/babel src --out-dir lib",
    "build:esModules:watch": "npm run build:esModules -- --watch",
    "build:css": "npm run clean:css && npm run build:css:bundle",
    "build:css:bundle": "npm run css:sass && MINIFY=true npm run css:sass && npm run css:postcss",
    "build:var": "npm run clean:var && npm run build:var:bundle",
    "build:var:bundle": "./node_modules/.bin/webpack --config webpack.config.var.js && MINIFY=true ./node_modules/.bin/webpack --config webpack.config.var.js",
    "build:storybook": "BABEL_ENV=production build-storybook -c .storybook -o test_reports/storybook-static",
    "clean": "npm run clean:test && npm run clean:esModules && npm run clean:var",
    "clean:esModules": "rimraf lib/*",
    "clean:css": "rimraf assets/*",
    "clean:test": "rimraf test_reports/*",
    "clean:var": "rimraf dist/*",
    "css:sass": "node sass.config.js",
    "css:postcss": "./node_modules/.bin/postcss -r assets/*.css",
    "component:new": "node scripts/new-component",
    "test": "npm run clean:test && npm run lint && npm run flow && npm run jest",
    "flow": "./node_modules/.bin/flow",
    "jest": "JEST_JUNIT_OUTPUT=test_reports/junit.xml ./node_modules/.bin/jest --config ./jest.config.json",
    "jest:watch": "npm run test -- --watch",
    "lint": "npm run lint:src",
    "lint:src": "./node_modules/.bin/eslint src --ext .js,.jsx",
    "lint:fix": "./node_modules/.bin/eslint src --ext .js,.jsx --fix",
    "storybook": "start-storybook -p 6006 --dont-track"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snapwagon/plugin-FE.git"
  },
  "keywords": [
    "coupons",
    "ftw"
  ],
  "author": "John Yeglinski <john@snapwagon.io>",
  "license": "Commercial License",
  "bugs": {
    "url": "https://github.com/snapwagon/plugin-FE/issues"
  },
  "homepage": "https://github.com/snapwagon/plugin-FE#readme",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.4.0",
    "@kadira/storybook": "2.35.3",
    "@kadira/storybook-addon-knobs": "1.7.1",
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "cssnano": "3.10.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "expect": "1.20.2",
    "file-loader": "0.11.1",
    "flow-bin": "0.43.1",
    "html-webpack-plugin": "^2.28.0",
    "inline-css-webpack-loader": "^1.0.0",
    "jest": "19.0.2",
    "jest-junit": "1.4.0",
    "jest-serializer-enzyme": "1.0.0",
    "lodash": "^4.17.4",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "mustache": "2.3.0",
    "node-sass": "4.5.2",
    "postcss": "5.2.17",
    "postcss-cli": "3.1.1",
    "postcss-loader": "1.3.3",
    "postcss-scss": "0.4.1",
    "precss": "1.4.0",
    "promisify-node": "0.4.0",
    "react-test-renderer": "15.5.4",
    "recompose": "0.23.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "isomorphic-fetch": "2.2.1",
    "load-segment": "1.0.1",
    "marked": "0.3.6",
    "node-sass": "4.5.2",
    "preact": "8.1.0",
    "preact-compat": "3.16.0",
    "prop-types": "15.5.10",
    "react-slick": "0.14.02",
    "react-stripe-elements": "0.0.2",
    "rollbar": "2.1.0",
    "stripe": "4.23.1",
    "to-markdown": "3.0.4"
  }
}
